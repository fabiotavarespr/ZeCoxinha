        </activity>
        <activity android:name=".SanduicheActivity" />
        <activity android:name=".AcompanhamentoActivity" />
        <activity android:name=".BebidaActivity" />
        <activity android:name=".FormaPagamentoActivity" />
        <activity android:name=".FinalizarActivity" />
    </application>

</manifest>